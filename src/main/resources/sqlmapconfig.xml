<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias alias="User" type="com.analysys.kong.model.User" />
		<typeAlias alias="Apis" type="com.analysys.kong.model.Apis" />
		<typeAlias alias="Consumers" type="com.analysys.kong.model.Consumers" />
		<typeAlias alias="ApiConsumers" type="com.analysys.kong.model.ApiConsumers" />
		<typeAlias alias="ApiLogs" type="com.analysys.kong.model.ApiLogs" />
		<typeAlias alias="ApioAuth" type="com.analysys.kong.model.ApioAuth" />
	</typeAliases>

	<mappers>
		<mapper resource="com/analysys/kong/dao/mapper/user.xml" />
		<mapper resource="com/analysys/kong/dao/mapper/apis.xml" />
		<mapper resource="com/analysys/kong/dao/mapper/consumers.xml" />
	</mappers>

</configuration>